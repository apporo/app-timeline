<!doctype html>
<html lang="en" class="no-js">
<head>
  <meta charset="UTF-8">
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-touch-fullscreen" content="yes">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
<%- include('../partials/header.style.ejs') %>
  <link rel="stylesheet" href="<%- contextPath %>/assets/layout/css/basic.css">
  <link rel="stylesheet" href="<%- contextPath %>/assets/tabbedbar/css/style.css">
  <title><%- __('Other Information') %> | <%- __('HISTORICAL TIMELINE') %></title>
</head>

<body>
<%- include('../partials/header.ejs') %>
  <main class="timeline-main cd-main-content">
    <h1><%- __('Other Information') %></h1>
    <% if (tabs) { %>
    <div class="cd-tabs">
      <nav>
        <ul class="cd-tabs-navigation">
          <% tabs.forEach(function(tab) { %>
          <li><a data-content="<%- tab.name %>" href="#0"<%- (tab.selected) ? ' class="selected"' : '' %>><%- tab.label %></a></li>
          <% }); %>
        </ul> <!-- cd-tabs-navigation -->
      </nav>

      <ul class="cd-tabs-content">
        <% tabs.forEach(function(tab) { %>
        <li data-content="<%- tab.name %>"<%- (tab.selected) ? ' class="selected"' : '' %>>
          <%- tab.content %>
        </li>
        <% }); %>
      </ul> <!-- cd-tabs-content -->
    </div> <!-- cd-tabs -->
    <% } %>
  </main>
<%- include('../partials/header.script.ejs') %>
  <script src="<%- contextPath %>/assets/tabbedbar/js/main.js"></script>
</body>
</html>
